<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\backend_API.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
	</system.webServer>
	  <system.webServer>
		  <httpErrors>
			  <remove statusCode="400" subStatusCode="402"/>
			  <clear/>
			  <error statusCode="404" path="/Forms" responseMode="Redirect" subStatusCode="400"/>
		  </httpErrors>
		  <applicationInitialization remapManagedRequestsTo="/"/>
		  <httpProtocol>
			  <customHeaders>
				  <remove name="Access-Control-Allow-Origin"/>
				  <clear/>
				  <add name="Access-Control-Allow-Origin" value="*"/>
				  
			  </customHeaders> 
		  </httpProtocol>
	  </system.webServer>
  </location>
</configuration>
